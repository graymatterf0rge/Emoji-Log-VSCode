{
	"name": "gitmoji-log-vscode",
	"displayName": "Gitmoji Log",
	"description": "Gitmoji-Log - A commit labeling extension using the emojis.",
	"version": "1.0.0",
	"author": {
		"name": "Jessie Taylor",
		"email": "b3457@graymatterf0rge.com"
	},
	"publisher": "graymatterf0rge",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/graymatterf0rge/Gitmoji-Log-VSCode"
	},
	"engines": {
		"vscode": "^1.40.0"
	},
	"galleryBanner": {
		"color": "#fad000"
	},
	"keywords": [
		"Gitmoji Log",
		"Gitmoji Log VSCode",
		"git",
		"emoji",
		"gitmoji",
		"commit",
		"messages"
	],
	"categories": [
		"Other"
	],
	"icon": "images/icon.png",
	"activationEvents": [
		"onCommand:extension.GitmojiLog"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.GitmojiLog",
				"title": "Gitmoji Log: Create git commit messages using Gitmoji-style icons [‚ú®‚ö°üêõüìùüöÄ‚úÖüí•]",
				"icon": {
					"dark": "images/icon_dark.svg",
					"light": "images/icon_light.svg"
				}
			}
		],
		"menus": {
			"scm/title": [
				{
					"when": "scmProvider == git",
					"command": "extension.GitmojiLog",
					"group": "navigation"
				}
			]
		}
	},
	"badges": [
		
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0"
	}
}
